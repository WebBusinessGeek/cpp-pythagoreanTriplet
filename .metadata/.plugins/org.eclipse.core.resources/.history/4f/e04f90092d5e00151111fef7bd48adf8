#include "gtest/gtest.h"
#include "Algorithm.h"


TEST(EnsureTestsAreWorking, itShouldReturnTrue) {
	Algorithm alg = Algorithm();
	bool result = alg.getTestStatus();
	bool correct = false;
	String incorrect = "bad";
	EXPECT_EQ(incorrect, result);
}
